<div>
<%= form_with model: @calendar, url:public_calendars_path, local: true do |f| %>
  <div class="title">
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>

    <div class="time">
    <%= f.label :start_time %>
    <%= f.datetime_select :start_time %>
  </div>

  <div class="content">
    <%= f.label :content %>
    <%= f.text_field :content %>
  </div>
<%= f.submit "保存", class:"btn btn-info col-3 m-1" %>
  
<% end %>
</div>